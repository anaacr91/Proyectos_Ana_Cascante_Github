<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sopaletrasgui.OldGameController">

   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Old Games by ID" />
      <ListView fx:id="ID" prefHeight="100.0" prefWidth="200.0">
         <VBox.margin>
            <Insets left="50.0" right="50.0" />
         </VBox.margin>
      </ListView>
      <TextField fx:id="gameid" alignment="CENTER" prefHeight="25.0" prefWidth="302.0" promptText="Enter the game id">
         <VBox.margin>
            <Insets left="75.0" right="75.0" />
         </VBox.margin>
      </TextField>
      <Button fx:id="searchid" mnemonicParsing="false" onAction="#onMouseClick3" text="Search" />
   </children>
</VBox>
